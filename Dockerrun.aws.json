{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
      "name": "client",
      "image": "ramakishore/new-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "ramakishore/new-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "ramakishore/new-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "ramakishore/new-nginx",
      "essential": true,
      "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client", "server"]

    }
  ]

}